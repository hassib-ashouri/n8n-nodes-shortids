    "description": "This is a node to generate url friendly and short uuids. This is useful when creating short urls. It is based on nonoid library.",
    "license": "MIT",
    "homepage": "https://github.com/hassib-ashouri/n8n-nodes-shortids#readme",
    "keywords": [
        "n8n-community-node-package"
    ],
    "author": {
        "name": "Hassib Ashouri",
        "email":"hassib.carbon492@slmails.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hassib-ashouri/n8n-nodes-shortids.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
